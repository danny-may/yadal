    "."
  ],
  "devDependencies": {
    "@types/node": "^20.5.7",
    "c8": "^8.0.1",
    "lerna": "^7.2.0",
    "typescript": "^5.2.2",
    "websocket": "^1.0.34",
    "ws": "^8.13.0"
  },
  "dependencies": {
    "@yadal/core": "workspace:^",
    "@yadal/gateway": "workspace:^",
    "@yadal/rest": "workspace:^"
