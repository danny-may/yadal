{
  "name": "yadal",
  "version": "1.0.0-alpha.5",
  "description": "Yet another discord api library",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danny-may/yadal.git"
  },
  "keywords": [
    "discord"
  ],
  "author": "danny-may",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/danny-may/yadal/issues"
  },
  "scripts": {
    "build:all": "lerna run build && tsc --noemit",
    "watch": "tsc --noemit -w",
    "build": "node ./build.mjs",
    "test": "lerna run build && tsc --noemit && c8 node --test"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    }
  },
  "homepage": "https://github.com/danny-may/yadal#readme",
  "workspaces": [
    "packages/*",
    "."
  ],
  "devDependencies": {
    "ws": "^8.13.0",
    "websocket": "^1.0.34",
    "why-is-node-running": "^2.2.2",
    "esbuild": "^0.17.18",
    "c8": "^7.14.0",
    "glob": "^10.2.6",
    "lerna": "^6.6.2",
    "typescript": "^5.0.4",
    "@types/node": "^18.11.9"
  },
  "dependencies": {
    "@yadal/core": "^1.0.0",
    "@yadal/rest": "^1.0.0",
    "@yadal/gateway": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}