{
  "name": "yadal",
  "version": "1.0.0-alpha.5",
  "description": "Yet another discord api library",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/danny-may/yadal.git"
  },
  "keywords": [
    "discord"
  ],
  "author": "danny-may",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/danny-may/yadal/issues"
  },
  "scripts": {
    "build:all": "lerna run build",
    "watch": "tsc -w",
    "build": "tsc",
    "test": "lerna run build && c8 node --test"
