{
    "javascript.preferences.importModuleSpecifierEnding": "js",
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js, $(capture).cjs, $(capture).mjs, $(capture).d.ts",
        "*.cjs": "$(capture).cjs.map",
        "*.mjs": "$(capture).mjs.map",
        "package.json": "pnpm-lock.yaml, pnpm-workspace.yaml, package-lock.json"
    },
    "files.exclude": {
        "**/.nyc_output": true,
        "**/node_modules": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "coverage": true,
        "**/*.js": true,
        "**/*.js.map": true
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "yaml.schemas": {
        "https://www.asyncapi.com/schema-store/all.schema-store.json": "file:///workspaces/yadal/packages/discord-spec/schemas/gateway.yaml"
    }
}