{
  "name": "@yadal/rest",
  "version": "1.0.0-alpha.4",
  "description": "Yet another discord api rest client",
  "author": "Danny May <dannyjmay97@gmail.com>",
  "homepage": "https://github.com/danny-may/yadal/tree/main/packages/rest#readme",
  "license": "SEE LICENSE IN LICENSE",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "esbuild src/index.ts --bundle --packages=external --external:*.json --loader:.ts=ts --outdir=dist --sourcemap --format=cjs --out-extension:.js=.cjs",
    "build:esm": "esbuild src/index.ts --bundle --packages=external --external:*.json --loader:.ts=ts --outdir=dist --sourcemap --format=esm --out-extension:.js=.mjs",
    "watch": "tsc --noemit -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danny-may/yadal.git"
  },
  "bugs": {
    "url": "https://github.com/danny-may/yadal/issues"
  },
  "dependencies": {
    "@yadal/dep": "^1.0.0-alpha.3",
    "discord-api-types": "^0.37.41"
  },
  "publishConfig": {
    "access": "public"
  }
}
